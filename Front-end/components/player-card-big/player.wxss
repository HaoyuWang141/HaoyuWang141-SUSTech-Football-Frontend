/* components/palyer-card-big/player.wxss */
.player-card {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  flex-wrap: nowrap;
  padding: 20rpx 20rpx;
}

.img, .number, .name, .goals, .assists {
  flex-grow: 0;
  flex-shrink: 0;
  color: #666;
}

.img {
  width: 80rpx;
  height: 80rpx;
  border-radius: 50rpx;
}

.name {
  flex-basis: 40%;
  font-weight:bold;
}

.name, .goals, .assists{
  font-size: small;
}

.number {
  background-image: url("data:image/svg+xml;base64,");
}

